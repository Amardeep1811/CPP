#include <iostream>
using namespace std;

class Addition {
public:
    int add(int a, int b) {
        return a + b;
    }
};

class Subtraction {
public:
    int subtract(int a, int b) {
        return a - b;
    }
};

class Calculator : public Addition, public Subtraction {
};

int main() {
    Calculator calc;
    cout << "Addition: " << calc.add(5, 3) << endl;
    cout << "Subtraction: " << calc.subtract(5, 3) << endl;

    return 0;
}
