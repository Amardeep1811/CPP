#include <iostream>
using namespace std;

int main() {
    int i;
    int size = 10;
    int arry[size] = {2 , 4 , 5 , 6 , 45 , 34 , 21 , 43 , 54 };
    int pos = 5;
    int element = 90;

    cout << "The array before insertion : \n";
    for (i = 0 ; i < size ; i++){
        cout  << arry[i] << endl;
    }
    
    for (i = size; i > pos; i--) {
        arry[i] = arry[i - 1];
    }
    
    
    arry[pos] = element;
    
